<?xml version="1.0" encoding="utf-8"?>
<dictionary version="2.0">
  <name>en02 - words (1)</name>
  <options />
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>(?&lt;OJStylePrefix&gt;.[ \u00A0])([-\u2014])(?&lt;OJStyleSuffix&gt;[ \u00A0].)</find>
    <style>All</style>
    <description>find any character + white space or non-breaking space + &lt;dash&gt; or &lt;m-dash&gt; + white space or non-breaking space + any character / replace &lt;dash&gt; or &lt;m-dash&gt; by &lt;n-dash&gt;</description>
    <id>en02-001</id>
    <replace>–</replace>
    <normalisedFind>([-\u2014])</normalisedFind>
    <prefix>.[ \u00A0]</prefix>
    <suffix>[ \u00A0].</suffix>
    <normalisedReplace>\u2013</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="false" wordRule="true" highlightOnly="true">
    <find>^u8211</find>
    <style>All</style>
    <description>find any character + white space or non-breaking space + &lt;n-dash&gt; or &lt;n-dash&gt; + white space or non-breaking space + any character then Highlight</description>
    <id>en02-001f</id>
    <replace></replace>
    <normalisedFind>\u2013</normalisedFind>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false" onlyintable="true">
    <find>(--)</find>
    <style>All</style>
    <description>find -- in a table / replace by em-dash</description>
    <id>en02-001a</id>
    <replace>—</replace>
    <normalisedFind>(--)</normalisedFind>
    <normalisedReplace>\u2014</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>([a-zA-Z]) ([-\u2014]) ([a-zA-Z])</find>
    <style>All</style>
    <description>find "letters &lt;dash&gt; or &lt;m-dash&gt; letters", replace by "letters &lt;n-dash&gt; letters"</description>
    <id>en02-001a</id>
    <replace>$1 – $3</replace>
    <normalisedFind>([a-zA-Z]) ([-\u2014]) ([a-zA-Z])</normalisedFind>
    <normalisedReplace>$1 \u2013 $3</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>(\d) ([-\u2014]) ([a-z]{2})</find>
    <style>All</style>
    <description>find "figure &lt;dash&gt; 2 low-case letters", replace by "figure &lt;n-dash&gt; 2 low-case letters"</description>
    <id>en02-001b</id>
    <replace>$1 – $3</replace>
    <normalisedFind>(\d) ([-\u2014]) ([a-z]{2})</normalisedFind>
    <normalisedReplace>$1 \u2013 $3</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>(\d) ([-\u2014]) (A )</find>
    <style>All</style>
    <description>find "figure &lt;dash&gt; upper case letter &amp; white space", replace by "figure &lt;n-dash&gt; upper case letter &amp; white space"</description>
    <id>en02-001c</id>
    <replace>$1 – $3</replace>
    <normalisedFind>(\d) ([-\u2014]) (A )</normalisedFind>
    <normalisedReplace>$1 \u2013 $3</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>(\d) ([-\u2014]) ([a-zA-Z])</find>
    <style>All</style>
    <description>find "figure &lt;dash&gt; or &lt;m-dash&gt; letters", replace by "letters &lt;n-dash&gt; letters"</description>
    <id>en02-001d</id>
    <replace>$1 – $3</replace>
    <normalisedFind>(\d) ([-\u2014]) ([a-zA-Z])</normalisedFind>
    <normalisedReplace>$1 \u2013 $3</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false" hasFindFormat="true">
    <find>(?&lt;OJStylePrefix&gt;_SUP_\d_SUP_)([-\u2014]) ([a-zA-Z])</find>
    <style>All</style>
    <description>find "footnote figure &lt;dash&gt; or &lt;m-dash&gt; letters", replace by "letters &lt;n-dash&gt; letters"</description>
    <id>en02-001e</id>
    <replace>– $2</replace>
    <normalisedFind>([-\u2014]) ([a-zA-Z])</normalisedFind>
    <prefix>_SUP_\d_SUP_</prefix>
    <normalisedReplace>\u2013 $2</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bEU(-? ?)([69])\b</find>
    <style>All</style>
    <description>find "EU-6" "EU-9" or "EU 6" "EU 9", replace by "EU-6" "EU-9"</description>
    <id>en02-002</id>
    <replace>EU-$2</replace>
    <normalisedFind>\b&lt;EU(-? ?)([69])\b&gt;</normalisedFind>
    <normalisedReplace>EU-$2</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bEU(-? ?)1([02])\b</find>
    <style>All</style>
    <description>find "EU-1[02]" or "EU 1[02]", replace by "EU-1[02]"</description>
    <id>en02-003</id>
    <replace>EU-1$2</replace>
    <normalisedFind>\b&lt;EU(-? ?)1([02])\b&gt;</normalisedFind>
    <normalisedReplace>EU-1$2</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bEU(-? ?)([12]5)\b</find>
    <style>All</style>
    <description>find "EU-[12]5" or "EU [12]5", replace by "EU-[12]5"</description>
    <id>en02-004</id>
    <replace>EU-$2</replace>
    <normalisedFind>\b&lt;EU(-? ?)([12]5)\b&gt;</normalisedFind>
    <normalisedReplace>EU-$2</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bG(-? ?)7\b</find>
    <style>All</style>
    <description>find "G-7" or "G 7", replace by "G7"</description>
    <id>en02-005</id>
    <replace>G7</replace>
    <normalisedFind>\b&lt;G(-? ?)7\b&gt;</normalisedFind>
    <normalisedReplace>G7</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bG(-? ?)24\b</find>
    <style>All</style>
    <description>find "G-24" or "G 24, replace by "G24"</description>
    <id>en02-006</id>
    <replace>G24</replace>
    <normalisedFind>\b&lt;G(-? ?)24\b&gt;</normalisedFind>
    <normalisedReplace>G24</normalisedReplace>
  </rule>
  <rule active="false" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>€ ?(\d?)€?</find>
    <style>All</style>
    <description>Find "€ [+ figure]", replaced by "EUR [+ figure]" / deactivated at the request EN team because of a text where ‘€’ should not have been changed</description>
    <id>en02-007</id>
    <replace>EUR $1</replace>
    <normalisedFind>€ ?(\d?)€?</normalisedFind>
    <normalisedReplace>EUR $1</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bOJ Nos?\.? ([CL])\b</find>
    <style>All</style>
    <description>find "OJ No(s)(.) [CL]", replace by "OJ [CL]"</description>
    <id>en02-008</id>
    <replace>OJ $1</replace>
    <normalisedFind>\b&lt;OJ Nos?\.? ([CL])\b&gt;</normalisedFind>
    <normalisedReplace>OJ $1</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\bOJ Nos?\.?(\s)?(\d)</find>
    <style>All</style>
    <description>find "OJ No(s)(.)( )1", replace by "OJ 1"</description>
    <id>en02-009</id>
    <replace>OJ $2</replace>
    <normalisedFind>\b&lt;OJ Nos?\.?(\s)?(\d)</normalisedFind>
    <normalisedReplace>OJ $2</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\b(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+) (and|to) (\d+),(\d+)\b</find>
    <style>All</style>
    <description>find "[sections?|subsections?|chapters?|...] 1[1],1[1] and|to 1[1],1[1]", replace by [sections?|subsections?|chapters?|...] 1[1].1[1] and|to 1[1].1[1]</description>
    <id>en02-010</id>
    <replace>$1 $2.$3 $4 $5.$6</replace>
    <normalisedFind>\b&lt;(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+) (and|to) (\d+),(\d+)\b&gt;</normalisedFind>
    <normalisedReplace>$1 $2.$3 $4 $5.$6</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\b(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+) or (\d+),(\d+)\b</find>
    <style>All</style>
    <description>find "[sections?|subsections?|chapters?|...] 1[1],1[1] or 1[1],1[1]", replace by "[sections?|subsections?|chapters?|...] 1[1].1[1] or 1[1].1[1]"</description>
    <id>en02-011</id>
    <replace>$1 $2.$3 or $4.$5</replace>
    <normalisedFind>\b&lt;(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+) or (\d+),(\d+)\b&gt;</normalisedFind>
    <normalisedReplace>$1 $2.$3 or $4.$5</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\b(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+)\b</find>
    <style>All</style>
    <description>find "[sections?|subsections?|chapters?|...] 1[1],1[1]", replace by "[sections?|subsections?|chapters?|...] 1[1].1[1]"</description>
    <id>en02-012</id>
    <replace>$1 $2.$3</replace>
    <normalisedFind>\b&lt;(sections?|subsections?|chapters?|articles?|annex|table|paragraphs?|subparagraphs?|points?|class|classes) (\d+),(\d+)\b&gt;</normalisedFind>
    <normalisedReplace>$1 $2.$3</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="true" wildcard="false" wordRule="true">
    <find>Luxemburg</find>
    <style>All</style>
    <description>find "Luxemburg", replace by "Luxembourg"</description>
    <id>en02-013</id>
    <replace>Luxembourg</replace>
    <normalisedFind>Luxemburg</normalisedFind>
    <normalisedReplace>Luxembourg</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="false" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="false">
    <find>\b(\d+)[,:](\d+)( \((Brussels|Luxembourg) time\))</find>
    <style>All</style>
    <description>find "1[1][,:]1[1] (Brussels|Luxembourg time)", replace by "1[1].1[1] (Brussels|Luxembourg time)"</description>
    <id>en02-014</id>
    <replace>$1.$2$3</replace>
    <normalisedFind>\b&lt;(\d+)[,:](\d+)( \((Brussels|Luxembourg) time\))</normalisedFind>
    <normalisedReplace>$1.$2$3</normalisedReplace>
  </rule>
  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="true">
    <find>&lt;JO ([CL]) ([0-9]@)&gt;</find>
    <style>All</style>
    <description>find "JO [LC] 1", replace by "OJ [LC] 1"</description>
    <id>en02-015</id>
    <replace>OJ \1 \2</replace>
    <normalisedFind>\b&lt;JO ([CL]) (\d+)\b&gt;</normalisedFind>
    <normalisedReplace>OJ $1 $2</normalisedReplace>
  </rule>
</dictionary>