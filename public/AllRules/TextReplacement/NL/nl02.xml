  <rule active="true" caseSensitive="true" endOfParagraph="false" startOfParagraph="false" wholeWord="false" wildcard="true" wordRule="true">
    <find>([0-9]) °([NZ]B)&gt;</find>
    <style>All</style>
    <description>find "1 °[NZ]B", replace by "1° [NZ]B" </description>
    <id>nl02-001</id>
    <replace>\1° \2</replace>
    <normalisedFind>(\d) °([NZ]B)\b&gt;</normalisedFind>
    <normalisedReplace>$1° $2</normalisedReplace>
  </rule>
